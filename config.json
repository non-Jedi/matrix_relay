{
	"id": "matrix_relay",
	"url": "http://localhost:8000",
	"as_token": "sample_token",
	"hs_token": "sample_token",
	"sender_localpart": "matrix_relay",
	"namespaces": {
		"users": [
			{
				"exclusive": true,
				"regex": "@relay_*"
			},
			{
				"exclusive": true,
				"regex": "@matrix_relay*"
			}
		],
		"rooms": [],
		"aliases": []
	},

	"homeserver_addr": "https://localhost:8448",
	"relay": {
		"!foo:bar": [["@ex:user", "@bar:foo"], ["!bar:example.com", "!bar:fo"]],
		"!all:users.relayed": [[], ["!to:this.room"]]
	},
	"user_prefix": "_relay_"
}
